{
  "name": "xg-windsurf",
  "displayName": "XG-Windsurf",
  "description": "XG-Windsurf 激活管理工具",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "repository": {
    "url": "https://github.com/your-username/xg-windsurf",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "xg-windsurf",
          "title": "XG-Windsurf",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "xg-windsurf": [
        {
          "type": "webview",
          "id": "xg-windsurf.view",
          "name": "XG-Windsurf"
        }
      ]
    },
    "commands": [
      {
        "command": "xg-windsurf.refresh",
        "title": "刷新视图",
        "category": "XG-Windsurf"
      },
      {
        "command": "xg-windsurf.clearData",
        "title": "清除所有数据",
        "category": "XG-Windsurf"
      }
    ],
    "keybindings": [
      {
        "command": "xg-windsurf.refresh",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "activeViewlet == 'workbench.view.extension.xg-windsurf'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "webpack",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "webpack --mode development --watch --config ./webpack/extension.config.js",
    "watch:wv": "webpack serve --mode development --config ./webpack/webview.config.js",
    "build": "npm run clean && npm-run-all --parallel build:* && npm run clean:maps",
    "build:ext": "webpack --mode production --config ./webpack/extension.config.js",
    "build:wv": "webpack --mode production --config ./webpack/webview.config.js",
    "package": "vsce package --no-dependencies",
    "release": "npm version patch && vsce package --no-dependencies",
    "release:minor": "npm version minor && vsce package --no-dependencies",
    "release:major": "npm version major && vsce package --no-dependencies",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist out *.tsbuildinfo",
    "clean:maps": "rimraf dist/**/*.map",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@estruyf/vscode": "^1.1.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.71.0",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/vsce": "^3.7.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.1",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.4.1",
    "typescript": "^4.7.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "undici": "5.28.4"
    }
  }
}
